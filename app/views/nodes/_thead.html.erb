<thead>
  <tr>
    <%= render "nodes/thead/string", id: "vipnet_id", value: params[:vipnet_id], tabindex: 2, label: t(".id_label") %>
    <%# info %>
    <%= render "nodes/thead/space" %>
    <%= render "nodes/thead/string", id: "name", value: params[:name], tabindex: 3, label: t(".name_label") %>
    <%# availability %>
    <%= render "nodes/thead/space" %>
    <%= render "nodes/thead/string", id: "ip", value: params[:ip], tabindex: 4, label: t(".ip_label") %>
    <%= render "nodes/thead/string", id: "version", value: params[:version], tabindex: 5, label: t(".software_label") %>
    <%# history %>
    <%= render "nodes/thead/space" %>
    <%= render "nodes/thead/string", id: "created_first_at", value: params[:created_first_at], tabindex: 6, label: t(".created_label") %>
    <%= render "nodes/thead/string", id: "deleted_at", value: params[:deleted_at], tabindex: 7, label: t(".deleted_label") %>
    <%= render "nodes/thead/string", id: "ticket_id", value: params[:ticket_id], tabindex: 8, label: t(".ticket_id_label") %>
    <%= render "nodes/thead/search_button", tabindex: 9 %>
  </tr>
</thead>

<th class="nodes__th nodes__empty-space-cell">
  <div class="nodes__search-button">
    <button id="nodes__search-button" type="submit" class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-button--primary" tabindex="<%= tabindex %>">
      <i class="material-icons">search</i>
    </button>
    <div class="mdl-tooltip" for="nodes__search-button">
      <%= t(".label_search") %>
    </div>
  </div>
  <% if false %>
  <div class="nodes__search-button--clear">
    <label id="nodes__search-button--clear" class="mdl-button mdl-js-button mdl-button--icon">
      <i class="material-icons">clear_all</i>
    </label>
    <div class="mdl-tooltip" for="nodes__search-button--clear">
      <%= t(".label_clear") %>
    </div>
  </div>
  <% end %>
</th>

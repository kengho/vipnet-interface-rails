(function() {
  vipnetInterface.remoteStatus.renderStatus({
    parentId: "<%= @response[:parent_id] %>",
    status: "true",
    tooltipText: "<%= @response[:tooltip_text] %>",
    undo: "true",
    ids: ["<%= @response[:parent_id] %> div[name='info']"],
  });
  vipnetInterface.remoteStatus.renderInfo({
    parentId: "<%= @response[:parent_id] %>",
    html: "<%= j render 'nodes/row/info_block' %>",
    name: "<%= @response[:name] %>",
    vipnetId: "<%= @response[:vipnet_id] %>",
    history: "<%= @response[:history] %>",
    category: "<%= @response[:category] %>",
    network: "<%= @response[:network] %>",
    ips: "<%= @response[:ips] %>",
    accessips: "<%= @response[:accessips] %>",
    vipnetVersion: "<%= @response[:vipnet_version] %>",
    vipnetVersionHW: "<%= @response[:vipnet_version_hw] %>",
    createdAt: "<%= @response[:created_first_at] %>",
    deletedAt: "<%= @response[:deleted_at] %>",
    mftpServer: "<%= @response[:mftp_server] %>",
    ncc: "<%= @response[:ncc] %>",
  });
})();
